clink="C:\tools\clink\clink_x64.exe" $*

clear=cls
e.=explorer .
md=mkdir $*
ll=ls -g --group-directories-first -h -s -o --color $*
ls=ls --show-control-chars -F --color $*
wheref=dir "$1" /s /b

nv=nvim $*
db=nvim -c DBUI
obsidian=nvim -c "WorkspacesOpen personal"

gaa=git add .
gcm=git commit -m $*
gd=git diff $*
gl=git log --oneline --all --graph --decorate$*
gs=git status --short

jv=java $*
gw=gradlew $*
pa=php artisan $*
pn=pnpm $*
dev=pnpm run dev
pnr=pnpm run $*
